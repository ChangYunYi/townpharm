// carousel slide
.container
	.row
		.col-md-8
			#myCarousel.carousel.slide(data-ride='carousel')
				// Indicators
				ol.carousel-indicators
					li.active(data-target='#myCarousel', data-slide-to='0')
					li(data-target='#myCarousel', data-slide-to='1')
				// Wrapper for slides
				.carousel-inner(role='listbox')
					.item.active
						img(src='/images/medications.jpg', alt='Image', style="height: 300px")
						.carousel-caption
					.item
						img(src='/images/singapore-431421_640.jpg', alt='Image', style="height: 300px")
						.carousel-caption
		.col-md-4
			.well
				p Some text..
			.well
				p Upcoming Events..
			.well
				p Visit Our Blog
